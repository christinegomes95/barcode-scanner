{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mishi-pay"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/styles/bootstrap/css/bootstrap.min.css",
        "assets/styles/font-awesome-4.7.0/css/font-awesome.min.css",
        "assets/styles/iconic/css/material-design-iconic-font.min.css",
        "assets/styles/linearicons-v1.0.0/icon-font.min.css",
        "assets/styles/animate/animate.css",
        "assets/styles/css-hamburgers/hamburgers.min.css",
        "assets/styles/animsition/css/animsition.min.css",
        "assets/styles/select2/select2.min.css",
        "assets/styles/daterangepicker/daterangepicker.css",
        "assets/styles/slick/slick.css",
        "assets/styles/MagnificPopup/magnific-popup.css",
        "assets/styles/perfect-scrollbar/perfect-scrollbar.css",
        "assets/styles/util.css",
        "assets/styles/main.css"
      ],
      "scripts": [
        "assets/scripts/jquery/jquery-3.2.1.min.js",
        "assets/scripts/animsition/js/animsition.min.js",
        "assets/scripts/bootstrap/js/popper.js",
        "assets/scripts/bootstrap/js/bootstrap.min.js",
        "assets/scripts/select2/select2.min.js",
        "assets/scripts/daterangepicker/moment.min.js",
        "assets/scripts/daterangepicker/daterangepicker.js",
        "assets/scripts/slick/slick.min.js",
        "assets/scripts/slick-custom.js",
        "assets/scripts/parallax100/parallax100.js",
        "assets/scripts/MagnificPopup/jquery.magnific-popup.min.js",
        "assets/scripts/isotope/isotope.pkgd.min.js",
        "assets/scripts/sweetalert/sweetalert.min.js",
        "assets/scripts/perfect-scrollbar/perfect-scrollbar.min.js",
        "assets/scripts/main.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
